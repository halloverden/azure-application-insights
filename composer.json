{
    "name": "halloverden/aai-sdk",
    "description": "This project extends the Application Insights API surface to support PHP.",
    "type": "library",
    "keywords": ["log","logging","telemetry","insights","monitoring"],
    "homepage": "https://github.com/Microsoft/ApplicationInsights-PHP",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "evert/phpdoc-md" : "~0.0.7"
    },
    "autoload": {
        "psr-4": {
            "HalloVerden\\ApplicationInsights\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HalloVerden\\ApplicationInsights\\Tests\\": "Tests/"
        }
    }
}
